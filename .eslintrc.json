{
  "extends": [
    "next/core-web-vitals",
    "airbnb-typescript",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "import-newlines"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "off",
    "import-newlines/enforce": [
      "error",
      {
        "items": 8,
        "max-len": 100
      }
    ]
  }
}
