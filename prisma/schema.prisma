// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Emoji {
    id           Int     @id @default(autoincrement())
    emoji        String  @unique
    upvote_count Int     @default(1)
}

model Blog {
    id    Int    @id @default(autoincrement())
    slug  String @unique
    views Int    @default(0)
}
